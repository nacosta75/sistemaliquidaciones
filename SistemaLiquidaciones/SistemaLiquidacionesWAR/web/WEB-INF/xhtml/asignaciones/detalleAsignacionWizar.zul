                                                                                                      </hbox>
                                                                                                      <intbox
                                                                                                          id="txtIdArticulo"
                                                                                                          hflex="1"
                                                                                                          maxlength="10"
                                                                                                          tooltiptext="Identificador unico de cliente"
                                                                                                      />
                                                                                                  </row>
                                                                                                  <row>
                                                                                                      <hbox>
                                                                                                          <label
                                                                                                              value="ICC: " />
                                                                                                          <label value="*" style="color:red; font-size:10px" />
                                                                                                      </hbox>
                                                                                                      <textbox  id="txtIcc" 
                                                                                                                hflex="1" />
                                                                                                  </row>                                                              

                                                                                                  <row>
                                                                                                      <hbox>
                                                                                                          <label value="IMEI" />
                                                                                                          <label value="*" style="color:red;font-size:10px" />
                                                                                                      </hbox>
                                                                                                      <textbox  
                                                                                                          id="txtImei" 
                                                                                                          hflex="1" />
                                                                                                  </row>
                                                                                                  <row>
                                                                                                      <hbox>
                                                                                                          <label value="Telefono:" />
                                                                                                          <label value="*" style="color:red;font-size:10px" />
                                                                                                      </hbox>
                                                                                                      <textbox  
                                                                                                          id="txtTelefono"
                                                                                                          hflex="1" />
                                                                                                  </row>
                                                                                              </rows>
                                                                                          </grid>
                                                                                      </groupbox>
                                                                                      <groupbox mold="3d" width="100%">
                                                                                          <caption label="Lista de articulos disponibles" />
                                                                                          
                                                                                          <listbox id="listBoxAticulos"
                                                                                                   style="border-top-width: 0px; border-left-width: 0px; border-right-width: 0px; border-bottom-width: 1px;"
                                                                                                   tooltiptext="Para ver detalle click sobre registro, para asignar registros click en boton a lado derecho"
                                                                                                   width="100%"
                                                                                                   height="100%"
                                                                                                   mold="paging"
                                                                                                   pagingPosition="top"
                                                                                                   multiple="true"
                                                                                                   checkmark="true"     pageSize="10"                                         >
                                                                                              <listhead sizable="true">
                                                                                                  <listheader id="listheaderLote"
                                                                                                              sclass="FDListBoxHeader1"
                                                                                                              sort="auto"
                                                                                                              width="25%"
                                                                                                              label="Id Lote" />
                                                                                                  <listheader id="listheaderArticulo"
                                                                                                              sclass="FDListBoxHeader1"
                                                                                                              sort="auto"
                                                                                                              width="25%"
                                                                                                              label="Articulo" />
                                                                                                  <listheader id="listheaderICC"
                                                                                                              sclass="FDListBoxHeader1"
                                                                                                              sort="auto"
                                                                                                              width="25%"
                                                                                                              label="ICC" />
                                                                                                  <listheader id="listheaderIMEI"
                                                                                                              sclass="FDListBoxHeader1"
                                                                                                              sort="auto"
                                                                                                              width="25%"
                                                                                                              label="IMEI" />
                                                                                                  <listheader id="listheaderTelefono"
                                                                                                              sclass="FDListBoxHeader1"
                                                                                                              sort="auto"
                                                                                                              width="25%"
                                                                                                              label="Telefono" />
                                                                                                 </listhead>
                                                                                          </listbox>
                                                                                      </groupbox>
                                             
                                        </panelchildren>
                                    </panel>
                                     </tabpanel>
                                     
                                     
                                        </tabpanels>                                 
                                    </tabbox>        
                                </div>
                                 
                            </vbox>
                        </cell>                                 
                    </hbox>                         
                </div>
                
            </center>
        </borderlayout>
    </window>
</zk>
