                                                                        <listheader id="listheaderPrecio"
                                                                                    sclass="FDListBoxHeader1"
                                                                                    sort="auto"
                                                                                    width="15%"
                                                                                    label="Precio" />
                                                                       </listhead>
                                                                </listbox>
                                                                
                                                            </groupbox>
                                                       
                                        </panelchildren>
                                      
                                    </panel>
                                    
                                     </tabpanel>
                                       
                                            <tabpanel style="color:#333399;" id="tabpanel3">
                                            <panel id="panelSelArt" border="none">                                                               
