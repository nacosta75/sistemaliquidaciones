                                                                         width="100%"
                                                                         height="90%"
                                                                         mold="paging"
                                                                         pagingPosition="top" pageSize="10"
                                                                         multiple="false">
                                                                    <listhead sizable="true">
                                                                        <listheader id="listheaderIdArticulo"
                                                                                    sclass="FDListBoxHeader1"
                                                                                    sort="auto"
                                                                                    width="10%"
                                                                                    label="Id" />
